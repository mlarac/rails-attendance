<%= form_for :event_attendance, url: {action: "create"} do |f| %>
    <%= f.select :event, options_for_select(Event.order('id DESC').all.collect{|p| [p.name, p.id]}) %>
    <%= f.select :member, options_for_select(Member.order('id DESC').all.collect{|p| [p.name, p.id]}) %>
<br>
    <%= f.submit 'Create Attendance' %>
<% end %>